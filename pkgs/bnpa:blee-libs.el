(defun bnpa:blee-libs/fullUpdate ()
 "blee-libs package adoption control."
 (interactive)
  (blee:ann|this-func (compile-time-function-name))
  (when bnpa:blee-libs:usgEnabled?
    (bnpa:blee-libs:install/update)
    (bnpa:blee-libs:config/main)
