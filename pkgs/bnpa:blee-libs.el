(defun bnpa:blee-libs:install/update ()
 "blee-libs package adoption install/update template."
 (interactive)
  (blee:ann|this-func (compile-time-function-name))
  (unless blee:dev:mode?
    (straight-use-package
     '(blee-libs :type git :host github :repo "bx-blee/blee-libs")))

  (when blee:dev:mode?
    (straight-use-package
     '(blee-libs :local-repo "/bisos/git/bxRepos/blee/blee-libs")))
  )
